<mjml>
  <mj-head>
    <mj-breakpoint width="768px" />
    <mj-attributes>
      <mj-class name="middle-col" vertical-align="middle" />
      <mj-class name="footer-col"
        font-size="16px"
        color="#555"
        line-height="24px"
      />
      <mj-class name="footer-wraper"
        background-color="#F8F1F4"
        padding="10px 20px"
      />
      <mj-all
        font-family="'Microsoft YaHei', 'Microsoft JhengHei', 'Noto-Sans-CJK-JP', 'Noto-Sans-Thai', sans-serif"
        font-size="14px"
        padding="0 0"
      />
    </mj-attributes>
    <mj-style>
      @media screen and (min-width: 768px) {
        .travel-info-area .banner-info div {
          font-size: 20px!important;
          line-height: 30px!important;
        }
        .travel-info-area .flight-item.flight-info .flight-desc {
          font-size: 16px!important;
          font-weight: bolder!important;
        }
        .travel-info-area .flight-item.flight-info .flight-sub {
          font-size: 16px!important;
        }
        .travel-info-area .flight-item.flight-info .left-line {
          width: 25%!important;
        }
        .travel-info-area .flight-item.flight-info .right-line {
          width: 75%!important;
        }
        .travel-info-area .flight-item.flight-info .line-msg {
          vertical-align: top!important;
        }
        .travel-info-area .flight-item.flight-info .from-area {
          width: 40%!important;
        }
        .travel-info-area .flight-item.flight-info .arrow {
          width: 7%!important;
        }
        .travel-info-area .flight-item.flight-info .to-area {
          width: 50%!important;
        }
        .travel-info-area .flight-item.flight-passenger .left-area b {
          font-size: 18px!important;
        }
        .travel-info-area .flight-item.flight-passenger .right-area,
        .travel-info-area .flight-item.flight-passenger .left-area {
          width: 48%!important;
          vertical-align: top!important;
          font-size: 16px!important;
        }
        .travel-info-area .flight-item.flight-passenger .ticket-no,
        .travel-info-area .flight-item.flight-passenger .ticket-status {
          width: 48%!important;
        }
        .travel-info-area .flight-item.flight-passenger .detail-elem {
          width: auto!important;
          padding-right: 20px!important;
        }
        .travel-info-area .flight-item.flight-passenger .hidden-br {
          display: block!important;
        }
      }
    </mj-style>
  </mj-head>

  <mj-body css-class="travel-info-area" width="1000px" background-color="#FAFAFA">
    <mj-section>
      <mj-column>
        <mj-text
          css-class="banner-info"
          line-height="21px"
          font-weight="600"
          font-size="14px"
          padding="30px 10px 40px"
        >
          <span>Your Excellency：</span>
          <br>
          <span>We </span><font color="#a70048">have checked in </font><span>online for your flight.</span>
          <br>
          <span>E-mail attachment for your E-boarding pass, I wish you a pleasant journey.</span>
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-section
      css-class="flight-item flight-info"
      background-url="https://yuuuki.gitee.io/tools.sitebuilder/demo.normal/images/bg-linear.jpg"
      background-repeat="no-repeat"
      background-size="100% 100%"
      padding="20px 10px"
    >
